<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<?xml-stylesheet type="text/xsl" href="callbacks.xsl"?>

<plugin name="Skel" short-name="Skel" author="federico.ferri.it@gmail.com">
    <description>An example plugin to demonstrate the usage of libPlugin</description>
    <command name="createObject">
        <description>Create an instance of the ExampleObject.</description>
        <params>
        </params>
        <return>
            <param name="handle" type="string">
                <description>handle of the newly created object</description>
            </param>
        </return>
    </command>
    <command name="destroyObject">
        <description>Create an instance of the ExampleObject.</description>
        <params>
            <param name="handle" type="string">
                <description>handle of the object</description>
            </param>
        </params>
        <return>
        </return>
    </command>
    <command name="setData">
        <description>Create an instance of the ExampleObject.</description>
        <params>
            <param name="handle" type="string">
                <description>handle of the object</description>
            </param>
            <param name="a" type="int">
                <description>an integer argument</description>
            </param>
            <param name="b" type="int">
                <description>an integer argument</description>
            </param>
        </params>
        <return>
        </return>
    </command>
    <command name="compute">
        <description>Compute some stuff using the data stored in the object.</description>
        <params>
            <param name="handle" type="string">
                <description>handle of the object</description>
            </param>
        </params>
        <return>
            <param name="currentSize" type="int">
                <description>Current size of the sequence</description>
            </param>
        </return>
    </command>
    <command name="getOutput">
        <description>Get output stored in the object.</description>
        <params>
            <param name="handle" type="string">
                <description>handle of the object</description>
            </param>
        </params>
        <return>
            <param name="output" type="table" item-type="int">
                <description>a vector of integers</description>
            </param>
        </return>
    </command>
</plugin>
